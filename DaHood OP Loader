LoadDahood.MouseButton1Click:Connect(function()
    local success, errorMessage = pcall(function()
        loadstring(game:HttpGet("", true))()
    end)
    
    if success then
        print("success")
    else
        warn("Failed to inject rage script:", errorMessage)
    end
end)
